    "import os\n",
    "import itertools"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "repeat  = 2\n",
    "file    = f\"PASSWORDS/{repeat}_all_passwords.txt\"\n",
    "num     = range(10)\n",
    "\n",
    "with open(file, \"wt\") as f:\n",
    "    for pw in itertools.product(num, repeat=repeat):\n",
    "        f.write(\"\".join([ str(x) for x in pw ]) + \"\\n\")"
   ]
  }
